{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".vscode": true,
        ".pytest_cache": true,
        "__pycache__": true,
        "tests/mylib/__pycache__": true,
        "imgui.ini": true,
        "pytest.ini": true,
        "requirements.txt": true,
        "mylib/__pycache__": true,
        "mylib/__init__.py": true,
        ".gitignore": true,
        "_experiments/__pycache__": true
    },
    "hide-files.files": [
        ".vscode",
        ".pytest_cache",
        "__pycache__",
        "tests/mylib/__pycache__",
        "imgui.ini",
        "pytest.ini",
        "requirements.txt",
        "mylib/__pycache__",
        "mylib/__init__.py",
        ".gitignore",
        "_experiments/__pycache__"
    ],
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}